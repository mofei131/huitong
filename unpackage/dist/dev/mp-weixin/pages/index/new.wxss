
.info{
	display: block;
}
uni-list-item{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding-bottom: 30rpx;
}
.listleft view{
	padding-bottom: 9rpx;
}
.listleft view:nth-child(1){
	color: #51565D;
	font-size: 32rpx;
	line-height: 45rpx;
}
.listleft view:nth-child(2){
	color: #9EA3A7;
	font-size: 24rpx;
}
.listright image{
	width: 220rpx;
	height: 124rpx;
	margin: 0 22rpx 0 22rpx;
}

